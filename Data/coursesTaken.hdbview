schema = "sot_schema";
query =  "select
	 T1.\"courseStatusID\",
	 T0.\"iNumber\",
	 T1.\"collegeName\",
	 T1.\"courseTaken\",
	 T1.\"collegeWebsite\" 
from \"sot_schema\".\"sot.Data::employeeDetails\" T0 
inner join \"sot_schema\".\"sot.Data::courseStatus\" T1 on T0.\"id\" = T1.\"id\" WITH READ ONLY";